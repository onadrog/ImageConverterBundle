{
    "name": "onadrog/imageconverterbundle",
    "description": "A simple FormType that convert images into WebP format",
    "type": "symfony-bundle",
    "keywords": [
        "image uploads",
        "converter",
        "form type"
    ],
    "authors": [
        {
            "name": "SÃ©bastien Gordano",
            "email": "sebastien.gordano@gmail.com",
            "homepage": "https://sebastiengordano.com"
        }
    ],
    "homepage": "https://github.com/onadrog/ImageConverterBundle",
    "require": {
        "symfony/uid": "^5.3",
        "symfony/form": "^5.3",
        "symfony/config": "^5.3",
        "symfony/event-dispatcher": "^5.3",
        "symfony/string": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "symfony/runtime": "^5.3",
        "symfony/mime": "^5.3",
        "twig/twig": "^3.3",
        "symfony/orm-pack": "^2.1",
        "symfony/property-access": "^5.3",
        "symfony/cache": "^5.3",
        "doctrine/orm": "^2.9",
        "doctrine/doctrine-bundle": "^2.4",
        "symfony/validator": "^5.3",
        "symfony/asset": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/phpstan": "^0.12.94",
        "phpstan/phpstan-symfony": "^0.12.41",
        "symfony/framework-bundle": "^5.3",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/yaml": "^5.3",
        "symfony/css-selector": "^5.3",
        "symfony/maker-bundle": "^1.33",
        "symfony/console": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/browser-kit": "^5.3"
    },
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Onadrog\\ImageConverterBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onadrog\\ImageConverterBundle\\Tests\\": "tests",
            "Onadrog\\ImageConverterBundle\\Mock\\": "tests/Mock"
        }
    },
    "suggest": {
        "symfony/maker-bundle": "Needed for command onadrog:make:entity",
        "symfony/yaml": "Needed for command onadrog:dump:config"
    },
    "support": {
        "wiki": "https://github.com/onadrog/ImageConverterBundle/wiki"
    },
    "minimum-stability": "stable"
}
